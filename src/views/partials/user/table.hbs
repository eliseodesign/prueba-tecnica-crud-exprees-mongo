<table>
  <thead>
    <tr>
      <th>N°</th>
      <th>Nombre</th>
      <th>Correo Electrónico</th>
      <th>Acciones</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}
    <tr> 
      <td>{{idUser}}</td>
      <td>{{nombre}}</td>
      <td>{{correo}}</td>
      <td>
        <a href="/user/{{_id}}/edit">Editar</a>
        <a href="/user/{{_id}}/delete">Eliminar</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
